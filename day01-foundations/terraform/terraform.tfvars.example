# Example terraform.tfvars file
# Copy this to terraform.tfvars and customize the values

# AWS region where resources will be created
aws_region = "us-east-1"

# Name for the IAM user (adjust as needed)
user_name = "aws-learning-user"

# Your email for resource tagging
owner_email = "your-email@example.com"

# Whether to create AWS Console access
create_console_access = true

# Require password reset on first login
require_password_reset = true
