# Example terraform.tfvars for Development Environment
# Copy this file to terraform.tfvars and customize the values

# AWS Configuration
aws_region  = "ap-south-1"
aws_profile = "sid-clouc-user"

# Project Configuration
project_name = "aws-learning"
owner_email  = "your-email@example.com"
cost_center  = "learning"

# Development Environment Configuration
vpc_cidr = "10.0.0.0/16"
availability_zones = ["ap-south-1a", "ap-south-1b"]

# Cost optimization for development
enable_nat_gateway = false
instance_type      = "t3.micro"

# Backend configuration (for reference - actual values used in backend.hcl)
backend_bucket         = "aws-learning-tfstate-603506755677-c977cb49"
backend_dynamodb_table = "aws-learning-terraform-locks"
backend_kms_key_id     = "arn:aws:kms:ap-south-1:603506755677:key/812f82ab-7d2a-4a23-b3b7-51a2f3274a46"
